#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/cameron/struct/docs/node_modules/.pnpm/@mintlify+cli@4.0.236_openapi-types@12.1.3_react-dom@18.3.1_react@18.3.1/node_modules/@mintlify/cli/bin/node_modules:/Users/cameron/struct/docs/node_modules/.pnpm/@mintlify+cli@4.0.236_openapi-types@12.1.3_react-dom@18.3.1_react@18.3.1/node_modules/@mintlify/cli/node_modules:/Users/cameron/struct/docs/node_modules/.pnpm/@mintlify+cli@4.0.236_openapi-types@12.1.3_react-dom@18.3.1_react@18.3.1/node_modules/@mintlify/node_modules:/Users/cameron/struct/docs/node_modules/.pnpm/@mintlify+cli@4.0.236_openapi-types@12.1.3_react-dom@18.3.1_react@18.3.1/node_modules:/Users/cameron/struct/docs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/cameron/struct/docs/node_modules/.pnpm/@mintlify+cli@4.0.236_openapi-types@12.1.3_react-dom@18.3.1_react@18.3.1/node_modules/@mintlify/cli/bin/node_modules:/Users/cameron/struct/docs/node_modules/.pnpm/@mintlify+cli@4.0.236_openapi-types@12.1.3_react-dom@18.3.1_react@18.3.1/node_modules/@mintlify/cli/node_modules:/Users/cameron/struct/docs/node_modules/.pnpm/@mintlify+cli@4.0.236_openapi-types@12.1.3_react-dom@18.3.1_react@18.3.1/node_modules/@mintlify/node_modules:/Users/cameron/struct/docs/node_modules/.pnpm/@mintlify+cli@4.0.236_openapi-types@12.1.3_react-dom@18.3.1_react@18.3.1/node_modules:/Users/cameron/struct/docs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@mintlify/cli/bin/index.js" "$@"
else
  exec node  "$basedir/../@mintlify/cli/bin/index.js" "$@"
fi
